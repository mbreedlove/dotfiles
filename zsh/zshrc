# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...

# Agnoster.zsh - requires powerline-patched font
# https://github.com/powerline/fonts
prompt agnoster

alias vi=vim

# load rbenv
eval "$(rbenv init -)"

# Homebrew GitHub API Token
export HOMEBREW_GITHUB_API_TOKEN="126bc8ac05de359af0f9861043d268c4c27e2769"

export NVM_DIR="/Users/mbreedlove/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"  # This loads nvm
